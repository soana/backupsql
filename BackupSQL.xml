<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>BackupSQL</Name>
  <Description>Cron backup of MySQL database.&#13;
Pleasae specify:&#13;
IP of the SQL server.&#13;
Cron parameters&#13;
Pushbullet token&#13;
Number of backup copies&#13;
</Description>
  <Registry>https://registry.hub.docker.com/u/soana/BackupSQL/</Registry>
  <Repository>soana/BackupSQL</Repository>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Environment>
    <Variable>
      <Name>bkpMinute</Name>
      <Value>0</Value>
    </Variable>
    <Variable>
      <Name>bkpHour</Name>
      <Value>3</Value>
    </Variable>
    <Variable>
      <Name>bkpDayOfMonth</Name>
      <Value>*</Value>
    </Variable>
    <Variable>
      <Name>bkpMonth</Name>
      <Value>*</Value>
    </Variable>
    <Variable>
      <Name>bkpDayOfWeek</Name>
      <Value>*</Value>
    </Variable>
    <Variable>
      <Name>MySQLIP</Name>
      <Value>192.168.0.xx</Value>
    </Variable>
    <Variable>
      <Name>TOKEN</Name>
      <Value>xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx</Value>
    </Variable>
    <Variable>
      <Name>BackupCopies</Name>
      <Value>7</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/Apps/MySQL/Backup/</HostDir>
      <ContainerDir>/backup</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/Apps/MySQL/config/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>27eb8e69</Version>
  <WebUI></WebUI>
  <Banner>https://raw.githubusercontent.com/soana/BackupSQL/master/banner.png</Banner>
  <Icon>https://raw.githubusercontent.com/soana/BackupSQL/master/icon.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
